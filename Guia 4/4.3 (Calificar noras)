Proceso calificando_notas
	Escribir "Ingrese su calificación (entre 1.0 y 7.0): ";
	Leer nota;
	Mientras nota<1.0 o nota>7.0 Hacer
		Escribir "Ingrese nota nuevamente:";
		Leer nota;
	FinMientras
	Si nota>=1.0 y nota<4.0 Entonces
		Escribir "Insuficiente";
	FinSi
	Si nota>=4.0 y nota<5.0 Entonces
		Escribir "Suficiente";
	FinSi
	Si nota>=5.0 y nota<6.0 Entonces
		Escribir "Bien";
	FinSi
	Si nota>=6.0 y nota<=7.0 Entonces
		Escribir "Muy bien";
	FinSi
	
	
	
	
FinProceso
